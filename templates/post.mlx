open JSX

let render ~meta (s: Data.Post.t) =
  <Layout meta=(meta) title=(s.title ^ " - Parsing Life")>
    [
      <div class_="container mx-auto px-6 py-12">
        Layout.back_button

        <img class_="my-6" src=("/" ^ s.image_src)/>
        <div class_="prose my-12">
            <h1>(string s.title)</h1>

            (s.html_content |> unsafe)
        </div>
      </div>
    ]
  </Layout>
